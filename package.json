{
  "name": "elk-widget",
  "version": "0.1.1",
  "description": "ELK widget for Jupyter",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets"
  ],
  "homepage": "https://github.gatech.edu/dfreeman6/elk-widget",
  "bugs": {
    "url": "https://github.gatech.edu/dfreeman6/elk-widget/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Dane Freeman",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.gatech.edu/dfreeman6/elk-widget.git"
  },
  "scripts": {
    "bootstrap": "jlpm --ignore-optional --ignore-scripts && jlpm clean && jlpm schema && jlpm lint && jlpm build",
    "build": "tsc -b",
    "clean": "rimraf lib ./elk/schema/elkschema.json",
    "watch": "jlpm build -w",
    "schema": "ts-json-schema-generator --path ./src/elkschema.ts -o ./elk/schema/elkschema.json",
    "lint": "jlpm prettier",
    "prettier": "prettier --write \"*.{json,yml,md}\" \"{src,style,elk}/**/*.{ts,tsx,js,jsx,css,json,md,yml}\""
  },
  "dependencies": {
    "@jupyterlab/application": "^1.2.7",
    "reflect-metadata": "^0.1.13",
    "sprotty-elk": "0.8.0",
    "worker-loader": "^2.0.0"
  },
  "devDependencies": {
    "@jupyter-widgets/base": "^2.0.1",
    "@jupyter-widgets/controls": "^1.5.1",
    "@jupyter-widgets/jupyterlab-manager": "^1.0.1",
    "@types/d3": "^5.0.0",
    "react": "*",
    "rimraf": "^2.6.1",
    "snabbdom": "~0.6.6",
    "ts-json-schema-generator": "^0.56.0",
    "typescript": "~3.5.2",
    "prettier": "^1.9.1"
  },
  "peerDependencies": {
    "@jupyter-widgets/base": "^2.0.1",
    "@jupyter-widgets/controls": "^1.5.1",
    "@jupyter-widgets/jupyterlab-manager": "^1.0.1",
    "react": "*"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": "lib/plugin"
  }
}
