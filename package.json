{
  "name": "elk-widget",
  "version": "0.1.0",
  "description": "ELK widget for Jupyter",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets"
  ],
  "homepage": "https://github.gatech.edu/dfreeman6/elk-widget",
  "bugs": {
    "url": "https://github.gatech.edu/dfreeman6/elk-widget/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Dane Freeman",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.gatech.edu/dfreeman6/elk-widget.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w",
    "schema": "ts-json-schema-generator --path ./node_modules/elkjs/lib/main.d.ts -o elkschema.json"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^2.0.1",
    "@jupyter-widgets/controls": "^1.5.1",
    "@jupyterlab/application": "^1.0.0",
    "react": "~16.8.4",
    "sprotty-elk": "0.7.0",
    "reflect-metadata": "^0.1.13",
    "webworker-threads": "^0.7.17"
  },
  "devDependencies": {
    "@types/d3": "^5.0.0",
    "rimraf": "^2.6.1",
    "ts-json-schema-generator": "^0.56.0",
    "typescript": "~3.5.2"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": "lib/plugin"
  }
}
